CREATE TABLE User (
 userID integer PRIMARY KEY,
 coupon integer,
 FOREIGN KEY (coupon) REFERENCES coupons (coupon)
 FOREIGN KEY (UserID) REFERENCES UserIDs (UserID)
);

CREATE TABLE UserIDs (
userID integer PRIMARY KEY
);

CREATE TABLE coupons (
 couponID integer PRIMARY KEY AUTO_INCREMENT,
 expirationDate text,
 type integer,
 value integer,
 
);

 CREATE TABLE UserTokens (
 UserID integer PRIMARY KEY.
 Tokens integer,
 Points integer,
 );

 CREATE TABLE UsedCoupons (
 userID integer PRIMARY KEY,
 couponID integer,
 expirationDate text,
 dateOfUse text,
 type integer,
 value integer
 );